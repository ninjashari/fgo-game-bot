<?xml version="1.0" encoding="utf-8"?>
<!--
    FGO Bot - Accessibility Service Configuration
    
    This file configures the accessibility service capabilities and permissions
    required for the FGO Bot automation functionality.
-->
<accessibility-service xmlns:android="http://schemas.android.com/apk/res/android"
    android:accessibilityEventTypes="typeAllMask"
    android:accessibilityFeedbackType="feedbackGeneric"
    android:accessibilityFlags="flagDefault|flagRetrieveInteractiveWindows|flagReportViewIds"
    android:canPerformGestures="true"
    android:canRetrieveWindowContent="true"
    android:description="@string/accessibility_service_description"
    android:notificationTimeout="100"
    android:packageNames="com.aniplex.fategrandorder,com.aniplex.fategrandorder.en" /> 